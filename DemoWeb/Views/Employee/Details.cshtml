@model IEnumerable<DemoApp.Model.DepartmentVM>

@{
    ViewData["Title"] = "Details";
}




<h4>Count of Departments</h4>
<hr />
<table class="table">
    <thead>
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.count)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var dept in Model)
        {
            <tr>
                <td>
                    @dept.Name
                </td>
                <td>
                    @dept.count
                </td>
            </tr>
        }
    </tbody>
</table>
<div>
    <a asp-action="Index" class="btn btn-primary">Back Home</a>
</div>
